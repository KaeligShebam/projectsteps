{% extends 'back/index.html.twig' %}

{% block title %}Ajouter une étape - {% block title_base %}{{ parent() }}{% endblock %}{% endblock %}

{% block body %}
	<div>
		{% block titre_admin_page %}Étape - Ajouter{% endblock %}
	</div>
	{% block admin_utilisateur %}
		<div class="was-validated form-back background-login is-valid">
				{{ form_start(form_steps_add_admin) }}
