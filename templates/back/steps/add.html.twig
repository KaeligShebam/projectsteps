{% extends 'back/index.html.twig' %}

{% block title %}Ajouter une étape - {% block title_base %}{{ parent() }}{% endblock %}{% endblock %}

{% block body %}
	<div>
		{% block titre_admin_page %}Étape - Ajouter{% endblock %}
	</div>
	{% block admin_utilisateur %}
		<div class="was-validated form-back background-login is-valid">
				{{ form_start(form_steps_add_admin) }}
			<div class="form-group">
				<div class="row">
					<div class="col">
						{{ form_row(form_steps_add_admin.customer) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.customerbrief) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.comingsoon) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.customercontentreception) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.picturesreception) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.webdesignprogress) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.webdesignwait) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.webdesignvalidated) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.webtraining) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.webintegration) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.online) }}
					</div>
					<div class="col">
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentcustomerbrief) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentcomingsoon) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentcustomercontentreception) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentpicturesreception) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentwebdesignprogress) }}
						<div class="mb-3"></div>
					</div>
					<div class="col">
						{{ form_row(form_steps_add_admin.commentwebdesignwait) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentwebdesignvalidated) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentwebintegration) }}
						<div class="mb-3"></div>
						{{ form_row(form_steps_add_admin.commentwebtraining) }}
						<div class="mb-3"></div>
					</div>
					{{ form_row(form_steps_add_admin.submit) }}
				</div>
			</div>
				{{ form_end(form_steps_add_admin) }}
		</div>
	{% endblock %}
{% endblock %}